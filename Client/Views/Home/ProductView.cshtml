@model List<Product>
@{
    ViewData["Title"] = "ProductView";
}

<h1>Просмотр таблицы</h1>

<table class="table">
    <thead>
        <th>Id</th>
        <th>Code</th>
        <th>Value</th>
    </thead>
    @foreach (Product country in Model)
    {
        <tr>
            <td>@country.Id</td>
            <td>@country.Code</td>
            <td>@country.Value</td>
        </tr>
    }
</table>